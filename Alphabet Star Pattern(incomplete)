#include<stdio.h>
int A()

{
    int i,j;
    for(i=1;i<=7;i++)
    {
        for(j=1;j<=5;j++)
        {
            if((i==1&&j>=1&&j<=5)||(j==1&&i>1)||(j==5&&i>1)||(i==4)||i==7)
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
int B()

{
    int i,j;
    for(i=1;i<=7;i++)
    {
        for(j=1;j<=5;j++)
        {
            if((i==1&&j>=1&&j<=5)||(j==1&&i>1)||(j==5&&(i==2||i==3||i==5||i==6))||(i==4&&j>1&&j<5)||i==7)
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
int E()

{
    int i,j;
    for(i=1;i<=7;i++)
    {
        for(j=1;j<=5;j++)
        {
            if((i==1&&j>=1&&j<=5)||(j==1&&i>1)||(i==4)||i==7)
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
