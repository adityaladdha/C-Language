#include <stdio.h>

int main()
{
    int a[2][2]={{5,5},{5,5}},b[2][2]={{6,6},{6,6}};
    int c[2][2];
    c[0][0]=a[0][0]+b[0][0];
    c[0][1]=a[0][1]+b[0][1];
    c[1][0]=a[1][0]+b[1][0];
    c[1][1]=a[1][1]+b[1][1];
    printf("The c matrix is:\n%d\t%d\n%d\t%d",c[0][0],c[0][1],c[1][0],c[1][1]);
    return 0;
}
