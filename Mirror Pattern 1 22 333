#include <stdio.h>

int main()
{
    int i,j,k,rows,c=1;
    printf("Enter the number of rows : ");
    scanf("%d",&rows);
    for(i=rows;i>=1;i--)
    {
        for(j=1;j<=i-1;j++)
        {
            printf(" ");
        }
        for(k=1;k<=c;k++)
        {
            printf("%d",c);
            
        }
        printf("\n");
        c++;
    }

    return 0;
}
