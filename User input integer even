#include <stdio.h>

int main()
{
    int n,a[100],i;
    printf("Enter the number of inputs : ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        printf("Enter number %d = ",i);
        scanf("%d",&a[i]);
    }
    printf("The even numbers are = ");
    for(i=1;i<=n;i++)
    {
        if(a[i]%2==0)
        printf("%d  ",a[i]);
        
    }
    
    return 0;
}
