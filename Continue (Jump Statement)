#include<stdio.h>
int main()
{
    int a=5,b,c,i,temp;
    int d[]={1,2,3,4,5,0,6};
    c=sizeof(d)/sizeof(d[0]);
    for(i=0;i<c;i++)
    {
        if(d[i]==0)
        {
            printf("Can't Divide by Zero \n");
            continue;
        }
        temp = a/d[i];
        printf("%d\n",temp);
    }
}
