#include<stdio.h>

void integer1()
{
    
    int i,j;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=4;j++)
        {
            if(j==2)
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }

}
void integer2()
{
      int i,j;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=4;j++)
        {
            if((i==1)||(j==4&&i>1&&i<4)||(i==3)||(j==1&&i>3&&i<=5)||(i==5))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
void integer3()
{
    int i,j;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=4;j++)
        {
            if((i==1)||(j==4&&i>1&&i<4)||(i==3)||(j==4&&i>3&&i<=5)||(i==5))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
void integer4()
{ 
    int i,j;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=4;j++)
        {
            if((j==1&&i>=1&&i<4)||(j==4&&i>=1&&i<4)||(i==3)||(j==4&&i>3&&i<=5))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }

}
void integer5()
{
    int i,j;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=4;j++)
        {
            if((i==1)||(j==4&&i>3&&i<=5)||(i==3)||(j==1&&i>1&&i<3)||(i==5))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }

}
int main()
{
    int n;
    printf("Enter your option : ");
    scanf("%d",&n);
    switch(n)
    {
        case 1 :integer1();break;
        case 2 :integer2();break;
        case 3 :integer3();break;
        case 4 :integer4();break;
        case 5 :integer5();break;
        default :printf("Invalid Input");
    }
    return 0;
}
