#include <stdio.h>

int main()
{
    int i,j,k,c,rows;
    printf("Enter the number of rows : ");
    scanf("%d",&rows);
    c=rows;
   for(i=1;i<=rows;i++)
   {
       for(j=1;j<=i;j++)
       {
           printf(" ");
       }
       for(k=1;k<=c;k++)
       {
           printf("%d",c);
       }
       c--;
       printf("\n");
    }
    return 0;
}
